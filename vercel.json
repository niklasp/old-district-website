{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --ignore-scripts",
  "framework": null,
  "build": {
    "env": {
      "NODE_VERSION": "16.20.2"
    }
  },
  "functions": {
    "server.js": {
      "runtime": "nodejs16.x"
    }
  },
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["build/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}
